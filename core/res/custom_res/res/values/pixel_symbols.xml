<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- These are system packages exempted from privacy indicator -->
    <java-symbol type="array" name="config_indicatorExemptedPackageNames" />

    <!-- PowerOffAlarmService -->
    <java-symbol type="string" name="notification_channel_poweroff_alarm" />
    <java-symbol type="string" name="poweroff_alarm_title" />
    <java-symbol type="string" name="poweroff_alarm_body" />
    <java-symbol type="drawable" name="ic_alarm_on" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Whether to use Richtap vibration -->
    <java-symbol type="bool" name="config_usesRichtapVibration" />

    <!-- Display color inversion availability -->
    <java-symbol type="bool" name="config_displayInversionAvailable" />

    <!-- The list of vendor package signatures that should also be considered
         platform signatures, specifically for use on devices with a vendor partition. -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- The list of Google platform package signatures
         that should also be considered platform signatures. -->
    <java-symbol type="array" name="config_googlePlatformSignatures" />
</resources>
