    <!-- Preferred max refresh rate at AOD & Ambient Display, if supported by the display. -->
    <integer name="config_aodMaxRefreshRate">-1</integer>

    <!-- Udfps vendor code -->
    <integer name="config_udfpsVendorCode">0</integer>
</resources>
